# Server Configuration
NODE_ENV=development
PORT=5000

# Database Configuration
DATABASE_URL=postgres://username:password@localhost:5432/build_performance

# API Configuration
API_ENDPOINT=http://localhost:5000/api/builds

# Provider Configuration
# Set PROVIDER_ENABLED=true to enable a provider

# Render
RENDER_ENABLED=true
RENDER_API_KEY=your_render_api_key
RENDER_API_ENDPOINT=https://api.render.com/v1
RENDER_REGION=oregon

# Vercel
VERCEL_ENABLED=false
VERCEL_API_KEY=your_vercel_api_key
VERCEL_API_ENDPOINT=https://api.vercel.com
VERCEL_TEAM_ID=your_team_id
VERCEL_PROJECT_ID=your_project_id

# Netlify
NETLIFY_ENABLED=false
NETLIFY_API_KEY=your_netlify_api_key
NETLIFY_API_ENDPOINT=https://api.netlify.com/api/v1
NETLIFY_SITE_ID=your_site_id

# AWS Amplify
AWS_AMPLIFY_ENABLED=false
AWS_ACCESS_KEY_ID=your_aws_access_key
AWS_SECRET_ACCESS_KEY=your_aws_secret_key
AWS_REGION=us-east-1
AWS_AMPLIFY_ENDPOINT=https://amplify.us-east-1.amazonaws.com
AWS_AMPLIFY_APP_ID=your_amplify_app_id

# GitHub Pages
GITHUB_PAGES_ENABLED=false
GITHUB_TOKEN=your_github_token
GITHUB_API_ENDPOINT=https://api.github.com
GITHUB_OWNER=your_github_username
GITHUB_REPO=your_repo_name
GITHUB_BRANCH=gh-pages

# Logging
LOG_LEVEL=debug 